{
    "name": "ulc-lvc-streamdeck-plugin",
    "version": "0.1.0",
    "description": "Stream Deck plugin for ULC and LVC (FiveM)",
    "type": "module",
    "scripts": {
        "build": "rollup -c rollup.config.mjs",
        "watch": "rollup -c rollup.config.mjs -w --watch.onEnd=\"streamdeck restart dev.sratzel.ulc-streamdeck-plugin\"",
        "bundle": "npm run build && node scripts/bundle.js"
    },
    "devDependencies": {
        "@elgato/cli": "^1.6.0",
        "@rollup/plugin-commonjs": "^28.0.0",
        "@rollup/plugin-node-resolve": "^15.2.2",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.0",
        "@tsconfig/node20": "^20.1.2",
        "@types/node": "~20.15.0",
        "@types/ws": "^8.18.1",
        "rollup": "^4.0.2",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "archiver": "^7.0.0"
    },
    "dependencies": {
        "@elgato/streamdeck": "^2.0.0",
        "ws": "^8.18.3"
    }
}